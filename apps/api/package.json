{
  "name": "@flagkit/api",
  "version": "0.1.0",
  "private": true,
  "description": "FlagKit API - Fastify backend server",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist",
    "analytics:generate": "tsx src/scripts/generate-sample-analytics.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "@fastify/jwt": "^7.2.4",
    "@fastify/multipart": "^8.1.0",
    "@fastify/websocket": "^9.0.0",
    "@paralleldrive/cuid2": "^3.0.4",
    "bcrypt": "^5.1.1",
    "fastify": "^4.25.2",
    "fastify-plugin": "^5.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@flagkit/config": "workspace:*",
    "@flagkit/database": "workspace:*",
    "@flagkit/types": "workspace:*",
    "@prisma/client": "^5.8.0",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^24.10.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitest/coverage-v8": "^4.0.8",
    "@vitest/ui": "^4.0.8",
    "eslint": "^8.56.0",
    "pino-pretty": "^10.3.1",
    "supertest": "^7.1.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.8"
  }
}
